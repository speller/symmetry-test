  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      }
    },
    "extends": [
      "standard",
      "standard-jsx",
      "eslint:recommended"
    ],
    "parser": "babel-eslint",
    "rules": {
      "indent": [
        "error",
        2
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "never"
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "eol-last": [
        "error",
        "never"
      ],
      "jsx-quotes": [
        "error",
        "prefer-double"
      ],
      "space-before-function-paren": [
        "error",
        "never"
      ]
